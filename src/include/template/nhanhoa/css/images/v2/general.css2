    padding: 0;
    margin: 0
}

* {
    padding: 0;
    margin: 0
}

a {
    text-decoration: none;
    color: #454545
}

a:hover {
    color: #FF7B00;
}

.clear {
    clear: both
}

input[type=text],input[type=password] {
    border: 1px #89b4d6 solid;
    color: #595959;
    margin: 0;
    padding: 1px
}

.bold {
    font-weight: bold
}

.green {
    color: #F77A00
}

.boldGreen {
    color: #F77A00;
    font-weight: bold
}

.center {
    text-align: center
}

.left {
    text-align: left
}

.right {
    text-align: right
}

table {
    border-collapse: collapse;
    table-layout: auto
}

